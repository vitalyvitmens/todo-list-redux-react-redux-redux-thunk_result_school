{
	"name": "my-app",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"prettier": "^2.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.0.5",
		"react-scripts": "5.0.1",
		"redux": "^4.2.1",
		"redux-thunk": "^2.4.2"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"serve": "json-server --watch src/db.json --port 8204"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest",
			"prettier"
		],
		"plugins": [
			"prettier"
		],
		"rules": {
			"react-hooks/exhaustive-deps": "off"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
